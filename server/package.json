{
  "name": "garaadka-server",
  "version": "1.0.0",
  "type": "module",
  "description": "Garaadka server API with TypeScript",
  "main": "index.ts",
  "scripts": {
    "server": "tsx index.ts",
    "dev": "NODE_ENV=development tsx watch index.ts",
    "start": "tsx index.ts",
    "start:prod": "NODE_ENV=production tsx index.ts",
    "build": "tsc",
    "test:customers": "node test-customers.js",
